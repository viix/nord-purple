		"settings": { "foreground": "#9699B0" }
	  },
	  {
		"scope": "constant.numeric",
		"settings": { "foreground": "#A397B2" }
	  },
	  {
		"scope": "storage.type",
		"settings": { "foreground": "#86809C" }
