{
	"name": "Nord Purple",
	"type": "dark",
	"colors": {
	  "editor.background": "#4A3F66",
	  "editor.foreground": "#E0E0E0",
	  "editorCursor.foreground": "#C792EA",
	  "editor.selectionBackground": "#5A4E80",
	  "editor.lineHighlightBackground": "#51476E",
	  "editorBracketMatch.background": "#5A4E80",
	  "editorBracketMatch.border": "#8A82AA",
	  "editorGutter.addedBackground": "#A3A1C2",
	  "editorGutter.deletedBackground": "#BFA1B6",
	  "editorGutter.modifiedBackground": "#C1A8C8",
	  "sideBar.background": "#4A3F66",
	  "sideBar.foreground": "#E0E0E0",
	  "sideBar.border": "#5A4E80",
	  "activityBar.background": "#4A3F66",
	  "activityBar.foreground": "#E0E0E0",
	  "activityBar.border": "#5A4E80",
	  "statusBar.background": "#4A3F66",
	  "statusBar.foreground": "#E0E0E0",
	  "statusBar.border": "#5A4E80",
	  "titleBar.activeBackground": "#4A3F66",
	  "titleBar.activeForeground": "#E0E0E0",
	  "titleBar.inactiveBackground": "#4A3F66",
	  "titleBar.inactiveForeground": "#B48EAD",
	  "tab.activeBackground": "#5A4E80",
	  "tab.inactiveBackground": "#4A3F66",
	  "tab.border": "#5A4E80",
	  "tab.unfocusedActiveBackground": "#4A3F66",
	  "tab.unfocusedInactiveBackground": "#4A3F66",
	  "editorGroupHeader.tabsBackground": "#4A3F66",
	  "editorGroupHeader.noTabsBackground": "#4A3F66",
	  "statusBarItem.remoteBackground": "#4A3F66",
	  "statusBarItem.remoteForeground": "#E0E0E0",
	  "list.activeSelectionBackground": "#5A4E80",
	  "list.inactiveSelectionBackground": "#4A3F66"
	},
	"tokenColors": [
	  {
		"scope": "comment",
		"settings": { "foreground": "#5E5C78", "fontStyle": "italic" }
	  },
	  {
		"scope": "string",
		"settings": { "foreground": "#A99BBC" }
	  },
	  {
		"scope": ["variable", "parameter"],
		"settings": { "foreground": "#D8DEE9" }
	  },
	  {
		"scope": "keyword",
		"settings": { "foreground": "#A092C6", "fontStyle": "bold" }
	  },
	  {
		"scope": "function",
		"settings": { "foreground": "#A3A7D2" }
	  },
	  {
		"scope": "constant.numeric",
		"settings": { "foreground": "#B89BC3" }
	  },
	  {
		"scope": "storage.type",
		"settings": { "foreground": "#8F8CB6" }
	  }
	]
  }
  